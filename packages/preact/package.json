{
  "name": "@fluxgpu/preact",
  "version": "5.1.0",
  "description": "Preact hooks and components for FluxGPU - lightweight alternative to @fluxgpu/react",
  "author": "AnNingUI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnNingUI/FluxGPU.git",
    "directory": "packages/preact"
  },
  "keywords": [
    "fluxgpu",
    "webgpu",
    "preact",
    "hooks",
    "components",
    "gpu",
    "graphics",
    "lightweight"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fluxgpu/contracts": "workspace:*",
    "@fluxgpu/engine": "workspace:*",
    "@fluxgpu/host-browser": "workspace:*"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "preact": "^10.19.0",
    "typescript": "^5.3.3"
  }
}
